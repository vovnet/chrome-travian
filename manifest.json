{
  "name": "Travian Bot",
  "description": "Farmer",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "storage"],
  "host_permissions": ["https://*.travian.com/*"],
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": ["https://*.travian.com/karte.php*"],
      "js": ["dist/main.js"]
    }
  ]
}
